package com.demo.stack;

public class MyStackList {
	Node top;
	class Node{
	private	Node next;
	private	int data;
	
	
	public Node(int data) {
		super();
		this.data = data;
		this.next = null;
	}
	}

	public MyStackList() {
		super();
		top  = null;
	}
	public boolean isFull()
	{
		if(top!=null)
		{
		return top.next==null;
		}
		return false;
	}
	
	public boolean isEmpty()
	{
		return top==null;
	}
	public boolean push(int value)
	{
		if(!isFull())
		{
		Node newNode = new Node(value);
		newNode.next = top;
		top = newNode;
		System.out.println("Added successfully");
		return true;
		}
		return false;
	}
	public int pop()
	{
		if(!isEmpty())
		{
			top=top.next;
			System.out.println("deleted successfully");
			return top.data;
		}
		return -1;
	}
  }
