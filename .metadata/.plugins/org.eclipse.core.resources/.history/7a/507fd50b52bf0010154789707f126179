package com.demo.stack;

public class MyQueueList {
Node front;
Node rear;
class Node {
	Node next;
	int data;
	public Node(int data) {
		super();
		this.data = data;
		this.next = null;
	}
	
}
public MyQueueList()
{
	front =null;
	rear = null;
}
public boolean isEmpty()
{
	return front==null && rear==null;
}
public boolean enqueue(int val)
{
	
	Node newNode = new Node(val);
	if(!isEmpty())
	{
	     Node temp =rear;
	     rear  = newNode;
	     temp.next= newNode;
	     System.out.println("added successfully");
			return true;
	}
		System.out.println("not added");
		return false;
}
public int dequeue()
{
	if(!isEmpty())
	{
		int val = front.data;
		front = front.next;
		System.out.println(val + " deleted successfully");
		return val;
	}
	System.out.println("not deleted cause it empty");
	return -1;
}
public void displayElements()
{
	Node temp = front;
	while(temp!=null)
	{
		temp = temp.next;
		System.out.print(temp.data + "-->");
	}
	System.out.println();
}

	
}
